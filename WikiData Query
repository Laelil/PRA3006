#PTSDtreatment
SELECT  ?drug ?drugLabel 
        ?drugClass ?drugClassLabel 
        ?drugFunction ?drugFunctionLabel 
        ?chemicalFormula 
        ?chemicalStructure ?chemicalStructureLabel

        ?proteinInteraction ?proteinInteractionLabel        
        ?otherDiseaseTreated ?otherDiseaseTreatedLabel 
        ?significantDrugInteractions ?significantDrugInteractionsLabel
        ?involvedInBiologicalProcess ?involvedInBiologicalProcessLabel

WHERE 
{
  wd:Q202387 wdt:P2176 ?drug.                                
  OPTIONAL { ?drug wdt:P279 ?drugClass. }                             
  OPTIONAL { ?drug wdt:P2868 ?drugFunction. } 
  OPTIONAL { ?drug wdt:P274 ?chemicalFormula. }
  OPTIONAL { ?drug wdt:P117  ?chemicalStructure. }                        
  
  OPTIONAL { ?drug wdt:P129 ?proteinInteraction. }
  OPTIONAL { ?drug wdt:P2175 ?otherDiseaseTreated. } 
  OPTIONAL { ?drug wdt:P769 ?significantDrugInteractions. }   
  OPTIONAL { ?drug wdt:P682 ?involvedInBiologicalProcess. }

 SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],en". } 
}
